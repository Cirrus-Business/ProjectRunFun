<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lizzy.runningapp_02" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".AndroidCode.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TownActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_town"
            android:theme="@style/FullscreenTheme" >
        </activity>
        <activity
            android:name=".AndroidCode.PlayerCreate"
            android:label="@string/title_activity_player_create" >
        </activity>
    </application>

</manifest>
